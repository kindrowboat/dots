#!/usr/bin/env bash

# Description of the unit
description() {
    echo "Description b"
}

# Dependencies for the unit (space-separated list)
dependencies() {
    echo "a"
}

# Main logic of the unit
main() {
    echo "running b"
}

# Ensure main function runs if the script is called directly
if [[ "${BASH_SOURCE[0]}" == "${0}" ]]; then
    main
fi
